    <selectionEntry id="3e2a-e0d1-0561-3872" name="Ka&apos;Bandha, Daemon General of Signus" book="HH8: Malevolence" hidden="false" collective="false" type="model">
      <profiles>
        <profile id="eb4a-79e6-b1eb-fc66" name="Ka&apos;Bandha, Daemon General of Signus" book="HH8: Malevolence" hidden="false" profileTypeId="556e697423232344415441232323" profileTypeName="Unit">
          <profiles/>
