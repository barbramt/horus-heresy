          <characteristics>
            <characteristic name="Unit Type" characteristicTypeId="556e6974205479706523232344415441232323" value="Flying Monstrous Creature (Character)"/>
            <characteristic name="WS" characteristicTypeId="575323232344415441232323" value="9"/>
